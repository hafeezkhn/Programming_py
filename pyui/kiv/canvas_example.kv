CanvasExample:

#:set s dp(150)
<CanvasExample>:
    canvas:
        Rectangle:
            pos:self.center_x - s/2,self.center_y -s/2
            size:s,s
        Ellipse:
            pos:200,100
            size:s,s/2
        Color:
            rgba:0,0,1,1
        Line:
            points:(0,0,100,20)
            width:5
